/**
 * Adds three 16-bit values.
 */
CHIP And{
     IN a,b;
     OUT out;
    //// Replace this comment with your code.
    PARTS:
    Nand(a=a,b=b,out=aNandb);
    Nand(a=aNandb,b=aNandb,out=out);
    
}
